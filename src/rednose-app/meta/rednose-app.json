{
    "rednose-app" : {
        "use": [
            "rednose-app-base",
            "rednose-model-undo",
            "rednose-model-spinner",
            "rednose-view-nav"
        ],

        "submodules": {
            "rednose-app-base" : {
                "group": "rednose-ui",
                "requires": [
                    "app-base",
                    "cssgrids",
                    "event-custom",
                    "handlebars-base",
                    "rednose-app-templates",
                    "rednose-panel"
                ],
                "skinnable": true
            },

            "rednose-model-undo" : {
                "group": "rednose-ui",
                "requires": [
                    "model"
                ]
            },

            "rednose-model-spinner" : {
                "group": "rednose-ui",
                "requires": [
                    "rednose-app-base",
                    "model"
                ]
            },

            "rednose-view-nav" : {
                "group": "rednose-ui",
                "requires": [
                    "event-custom",
                    "rednose-widget-nav-container",
                    "view"
                ]
            }
        }
    }
}
