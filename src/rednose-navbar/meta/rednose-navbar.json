{
    "rednose-navbar" : {
        "use": [
            "rednose-navbar-keys",
            "rednose-navbar-recent"
        ],

        "submodules": {
            "rednose-navbar-base" : {
                "requires": [
                    "json",
                    "node-pluginhost",
                    "rednose-dropdown-plugin",
                    "view"
                ]
            },

            "rednose-navbar-keys" : {
                "requires": [
                    "event-custom",
                    "rednose-dropdown-keys",
                    "rednose-navbar-base"
                ]
            },

            "rednose-navbar-recent" : {
                "requires": [
                    "cookie",
                    "plugin",
                    "rednose-navbar-base"
                ]
            }
        }
    }
}
